    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug BicepGen service",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/BicepGenerator/main.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        // {
        //     "name": "debug Bicep Gen",
        //     "type": "python",
        //     "request": "launch",
        //     "module": "flask",
        //     "env": {
        //         "FLASK_APP": "main",
